{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/decoration/$1"
        },
        {
            "source": "/ws",
            "destination": "/api/main.py"
        },
        {
            "source": "/command",
            "destination": "/api/main.py"
        },
        {
            "source": "/live.html",
            "destination": "/frontend/live.html"
        },
        {
            "source": "/mock.html",
            "destination": "/frontend/mock.html"
        },
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}